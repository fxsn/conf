# ls
alias ll='ls -lhF --color=auto --group-directories-first'
alias la='ls -lhAF --color=auto --group-directories-first'
alias lt="ls -lhAF --color=auto --sort=time"

# git
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias go='git checkout '
alias gk='gitk --all&'
alias gx='gitx --all'
